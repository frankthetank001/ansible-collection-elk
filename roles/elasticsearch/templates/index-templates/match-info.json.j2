{
  "template": {
    "settings": {
      "index": {
        "lifecycle": {
          "name": "match-info",
          "rollover_alias": "alias-match-info" 
        },
        "refresh_interval": "5s",
        "number_of_shards": "{{ elasticsearch_hosts | length }}",
        "number_of_replicas": "0-all"
      }
    },
    "aliases": {
      "alias-match-info": {
        "is_write_index": true 
      }
    },
    "mappings": {
      "_routing": {
        "required": false
      },
      "numeric_detection": false,
      "dynamic_date_formats": [
        "strict_date_optional_time",
        "yyyy/MM/dd HH:mm:ss Z||yyyy/MM/dd Z"
      ],
      "_source": {
        "excludes": [],
        "includes": [],
        "enabled": true
      },
      "dynamic": true,
      "dynamic_templates": [],
      "date_detection": true,
      "properties": {
        "Match": {
          "properties": {
            "Stats": {
              "properties": {
                "Exp_Earned": {
                  "type": "float"
                },
                "Damage": {
                  "properties": {
                    "Dealt": {
                      "type": "float"
                    }
                  }
                },
                "Gold_Earned": {
                  "type": "float"
                },
                "Kill": {
                  "properties": {
                    "Source": {
                      "properties": {
                        "Name": {
                          "type": "text",
                          "fields": {
                            "keyword": {
                              "type": "keyword",
                              "ignore_above": 256
                            }
                          }
                        },
                        "Type": {
                          "type": "text",
                          "fields": {
                            "keyword": {
                              "type": "keyword",
                              "ignore_above": 256
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            },
            "Players_Connected": {
              "type": "integer"
            },
            "Player": {
              "properties": {
                "Name": {
                  "type": "text",
                  "fields": {
                    "keyword": {
                      "type": "keyword",
                      "ignore_above": 256
                    }
                  }
                },
                "IP": {
                  "type": "ip"
                },
                "PSR": {
                  "coerce": true,
                  "index": true,
                  "ignore_malformed": false,
                  "store": false,
                  "type": "float",
                  "doc_values": true
                }
              }
            },
            "In_Progress": {
              "type": "boolean"
            },
            "ID": {
              "type": "text",
              "fields": {
                "keyword": {
                  "type": "keyword",
                  "ignore_above": 256
                }
              }
            },
            "Event": {
              "properties": {
                "Type": {
                  "type": "text",
                  "fields": {
                    "keyword": {
                      "type": "keyword",
                      "ignore_above": 256
                    }
                  }
                }
              }
            },
            "Name": {
                "type": "text",
                "fields": {
                    "keyword": {
                        "type": "keyword",
                        "ignore_above": 256
                    }
                }
            },
            "Mode": {
                "type": "text",
                "fields": {
                    "keyword": {
                        "type": "keyword",
                        "ignore_above": 256
                    }
                }
            },
            "Options": {
                "type": "text",
                "fields": {
                    "keyword": {
                        "type": "keyword",
                        "ignore_above": 256
                    }
                }
            }
          }
        },
        "Diagnostics": {
          "properties": {
            "ping": {
              "type": "integer"
            },
            "average_ping": {
              "type": "integer"
            },
            "clients_connected": {
              "type": "integer"
            },
            "client_ip": {
              "type": "ip"
            },
            "client_name": {
              "type": "keyword"
            },
            "reliable_sv_sent": {
              "type": "integer"
            },
            "reliable_cl_ack": {
              "type": "integer"
            },
            "reliable_cl_sent": {
              "type": "integer"
            },
            "reliable_sv_ack": {
              "type": "integer"
            },
            "unreliable_sv_sent": {
              "type": "integer"
            },
            "unreliable_cl_recv": {
              "type": "integer"
            },
            "unreliable_cl_sent": {
              "type": "integer"
            },
            "unreliable_sv_recv": {
              "type": "integer"
            },
            "load_average": {
              "type": "float"
            },
            "total_long_server_frames": {
              "type": "integer"
            }
          }
        },
        "geo": {
          "properties": {
            "client": {
              "properties": {
                "location": {
                  "type": "geo_point"
                }
              }
            },
            "host": {
              "properties": {
                "location": {
                  "type": "geo_point"
                }
              }
            }
          }
        },
        "Server": {
          "properties": {
            "Public_IP": {
              "type": "ip"
            },
            "Event": {
              "properties": {
                "Connection": {
                  "properties": {
                    "IP": {
                      "type": "ip"
                    }
                  }
                }
              }
            },
            "Connected": {
              "type": "boolean"
            },
            "Name": {
              "type": "text",
              "fields": {
                "keyword": {
                  "type": "keyword",
                  "ignore_above": 256
                }
              }
            }
          }
        },
        "client": {
          "properties": {
            "ip": {
              "type": "ip"
            },
            "geo": {
              "properties": {
                "location": {
                  "ignore_malformed": false,
                  "type": "geo_point",
                  "ignore_z_value": true
                }
              }
            }
          }
        },
        "host": {
          "properties": {
            "ip": {
              "type": "ip"
            },
            "geo": {
              "properties": {
                "location": {
                  "type": "geo_point"
                }
              }
            }
          }
        },
        "HoNfigurator": {
          "properties": {
            "Disable_Bots": {
              "type": "boolean"
            },
            "Auto-Update": {
              "type": "boolean"
            },
            "Proxy_Enabled": {
              "type": "boolean"
            },
            "Bots_Running": {
              "type": "boolean"
            },
            "Use_Static_IP": {
              "type": "boolean"
            }
          }
        },
        "match_name": {
            "type": "text",
            "fields": {
                "keyword": {
                    "type": "keyword",
                    "ignore_above": 256
                }
            }
        },
        "match_mode": {
            "type": "text",
            "fields": {
                "keyword": {
                    "type": "keyword",
                    "ignore_above": 256
                }
            }
        },
        "match_map": {
            "type": "text",
            "fields": {
                "keyword": {
                    "type": "keyword",
                    "ignore_above": 256
                }
            }
        },
        "match_options": {
            "type": "text",
            "fields": {
                "keyword": {
                    "type": "keyword",
                    "ignore_above": 256
                }
            }
        },
        "Player": {
          "type": "alias",
          "path": "Match.Player.Name.keyword"
        },
        "Name": {
          "type": "alias",
          "path": "Server.Name.keyword"
        },
        "Match_ID": {
          "type": "alias",
          "path": "Match.ID.keyword"
        },
        "Match_Event": {
          "type": "alias",
          "path": "Match.Event.Type.keyword"
        },
        "Killed_By": {
          "type": "alias",
          "path": "Match.Stats.Kill.Source.Name.keyword"
        }
      }
    }
  },
  "index_patterns": [
    "match-info-*"
  ],
  "composed_of": []
}