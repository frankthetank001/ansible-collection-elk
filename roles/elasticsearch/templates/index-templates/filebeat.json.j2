{
  "template": {
    "settings": {
      "index": {
        "lifecycle": {
          "name": "filebeat"
        },
        "refresh_interval": "5s",
        "number_of_shards": "{{ elasticsearch_hosts | length }}",
        "number_of_replicas": "{{ [elasticsearch_hosts | length - 1, 0] | max }}"
      }
    },
    "mappings": {
      "_routing": {
        "required": false
      },
      "numeric_detection": false,
      "dynamic_date_formats": [
        "strict_date_optional_time",
        "yyyy/MM/dd HH:mm:ss Z||yyyy/MM/dd Z"
      ],
      "_source": {
        "excludes": [],
        "includes": [],
        "enabled": true
      },
      "dynamic": true,
      "dynamic_templates": [],
      "date_detection": true,
      "properties": {
        "Match.Stats.Exp_Earned": {
          "type": "float"
        },
        "Match.Players_Connected": {
          "type": "integer"
        },
        "Match.Player.Name": {
          "type": "text",
          "fields": {
              "keyword": {
                  "type": "keyword",
                  "ignore_above": 256
              }
          }
        },
        "Match.Player.IP": {
          "type": "ip"
        },
        "Diagnostics": {
          "properties": {
            "ping": {
              "type": "integer"
            },
            "average_ping": {
              "type": "integer"
            },
            "clients_connected": {
              "type": "integer"
            },
            "client_ip": {
              "type": "ip"
            },
            "client_name": {
              "type": "keyword"
            },
            "reliable_sv_sent": {
              "type": "integer"
            },
            "reliable_cl_ack": {
              "type": "integer"
            },
            "reliable_cl_sent": {
              "type": "integer"
            },
            "reliable_sv_ack": {
              "type": "integer"
            },
            "unreliable_sv_sent": {
              "type": "integer"
            },
            "unreliable_cl_recv": {
              "type": "integer"
            },
            "unreliable_cl_sent": {
              "type": "integer"
            },
            "unreliable_sv_recv": {
              "type": "integer"
            },
            "load_average": {
              "type": "float"
            },
            "total_long_server_frames": {
              "type": "integer"
            }
          }
        },
        "geo.client.location": {
          "type": "geo_point"
        },
        "Match.Stats.Damage.Dealt": {
          "type": "float"
        },
        "Server.Public_IP": {
          "type": "ip"
        },
        "test.client.geo.location": {
          "type": "geo_point"
        },
        "Server.Event.Connection.IP": {
          "type": "ip"
        },
        "Match.Stats.Gold_Earned": {
          "type": "float"
        },
        "client.ip": {
          "type": "ip"
        },
        "HoNfigurator.Disable_Bots": {
          "type": "boolean"
        },
        "Server.Connected": {
          "type": "boolean"
        },
        "HoNfigurator.Auto-Update": {
          "type": "boolean"
        },
        "Match.In_Progress": {
          "type": "boolean"
        },
        "host.ip": {
          "type": "ip"
        },
        "client.geo.location": {
          "ignore_malformed": false,
          "type": "geo_point",
          "ignore_z_value": true
        },
        "geoip.location": {
          "type": "geo_point"
        },
        "testgeo.location": {
          "ignore_malformed": false,
          "type": "geo_point",
          "ignore_z_value": true
        },
        "Match.Player.PSR": {
          "coerce": true,
          "index": true,
          "ignore_malformed": false,
          "store": false,
          "type": "float",
          "doc_values": true
        },
        "HoNfigurator.Proxy_Enabled": {
          "type": "boolean"
        },
        "HoNfigurator.Bots_Running": {
          "type": "boolean"
        },
        "geo.host.location": {
          "ignore_malformed": false,
          "type": "geo_point",
          "ignore_z_value": true
        },
        "host.geo.location": {
          "type": "geo_point"
        },
        "HoNfigurator.Use_Static_IP": {
          "type": "boolean"
        }
      }
    }
  },
  "index_patterns": [
    "filebeat*"
  ],
  "data_stream": {
    "hidden": false,
    "allow_custom_routing": false
  },
  "composed_of": []
}